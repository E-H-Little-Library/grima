<GrimaForm>
    <Title>Grima: Change Storage Location ID</Title>
    <Field
        name="slid"
        label="Storage Location ID"
        shortname="s"
        autocomplete="on"
        required="no"
        persistent="yes"
        placeholder="Storage Location ID..."
    />
    <Field
        name="barcode"
        label="Barcode"
        shortname="b"
        autocomplete="off"
        required="yes"
        placeholder="Barcode of item to modify..."
    />
</GrimaForm>
